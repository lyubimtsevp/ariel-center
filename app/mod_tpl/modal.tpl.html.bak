				<?foreach(d()->ru_alf as $key=>$val):?>
					<div class="city_block">
					<strong class="b"><?print $val;?></strong>
					<?foreach(d()->cities_list as $k=>$v):?>
						<?if($val==$v['code'])print '<a href="?change_city='.$v['id'].'">'.$v['title'].'</a><br>';?>
					<?endforeach;?>
					</div>
				<?endforeach;?>
			@else:
				<?foreach(d()->en_alf as $key=>$val):?>
					<div class="city_block">
					<strong class="b"><?print $val;?></strong>
					<?foreach(d()->cities_list as $k=>$v):?>
						<?if($val==$v['en_code'])print '<a href="?change_city='.$v['id'].'">'.$v['en_title'].'</a><br>';?>
					<?endforeach;?>
					</div>
				<?endforeach;?>
			@endif;
		</div>
      </div>
    </div>
  </div>
</div>

